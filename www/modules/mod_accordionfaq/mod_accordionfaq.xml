	<params  addpath="/modules/mod_accordionfaq" >
		<param name="cssfile" type="text" default="modules/mod_accordionfaq/css/accordionfaq.css" label="CSS stylesheet" size="50" description="PARAMCSSSTYLESHEET" />
		<param name="includejquery" type="radio" default="2" label="Include jQuery script" description="PARAMINCLUDEJQUERY" >
			<option value="0">No</option>
			<option value="1">Yes</option>
			<option value="2">Yes(Minified version)</option>
		</param>
		<param name="faqid" type="text" default="accordion1" label="FAQ id" size="50" description="PARAMFAQID" />
		<param name="faqclass" type="text" default="" label="FAQ classes" size="50" description="PARAMFAQCLASS" />
		<param name="header" type="text" default="h3" label="Header element" size="50" description="PARAMHEADER" />
		<param name="autonumber" type="radio" default="fals" label="Auto number" description="PARAMAUTONUMBER" >
			<option value="fals">No</option>
			<option value="tru">Yes</option>
		</param>
		<param name="autoheight" type="radio" default="tru" label="Auto height" description="PARAMAUTOHEIGHT" >
			<option value="fals">No</option>
			<option value="tru">Yes</option>
		</param>
		<param name="alwaysopen" type="radio" default="tru" label="Always open" description="PARAMALWAYSOPEN" >
			<option value="fals">No</option>
			<option value="tru">Yes</option>
		</param>
		<param name="active" type="text" default="" label="Active element" size="50" description="PARAMACTIVE" />
		<param name="animation" type="list" default="slide" label="Animation" size="50" description="PARAMANIMATION" >
			<option value="none">None</option>
			<option value="easeslide">Ease Slide</option>
			<option value="bounceslide">Bounce Slide</option>
			<option value="slide">Slide</option>
		</param>
		<param name="event" type="list" default="click" label="Activate event" size="50" description="PARAMEVENT" >
			<option value="click">Single click</option>
			<option value="dblclick">Double click</option>
			<option value="mouseover">Mouseover</option>
		</param>
		<param name="article" type="sqllist" default="-1" query="SELECT a.id, a.title from #__content as a where a.state > 0 order by a.title" key_field="id" value_field="title" label="Article" description="PARAMARTICLE" />
		<param name="scrolltime" type="text" default="1000" label="Scroll time" size="50" description="PARAMSCROLLTIME" />
		<param name="scrolloffset" type="text" default="0" label="Scroll offset" size="50" description="PARAMSCROLLOFFSET" />
		<param name="warnings" type="radio" default="1" label="Issue warning messages" description="PARAMWARNINGS" >
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
	<params group="advanced">
		<param name="cache" type="list" default="0" label="Caching" description="Select whether to cache the content of this module">
			<option value="1">Use global</option>
			<option value="0">No caching</option>
		</param>
		<param name="cache_time" type="text" default="15" label="Cache Time" description="The time in minutes before the module is re-cached." />
	</params>
</install>